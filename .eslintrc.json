{
    "env": {
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    },
    "plugins": [
        "babel"
    ],
    "rules": {
        "no-console": 0,
        "indent": [ "error", 4 ],
        "no-param-reassign": [
            "error", { "props": false }
        ],
        "import/prefer-default-export": 0
    }
}